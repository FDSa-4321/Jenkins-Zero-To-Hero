pipeline{
    agent any
    stages{
        
      stage('Build'){
            steps{
                echo 'This is a Build Stage1'
            }
        } 
        
      stage('Test'){
            steps{
                echo 'This is a Test Stage'
            }
        }
        
      stage('Deploy'){
            steps{
                echo 'This is a Deploy Stage'
            }
        }
        
    }
}
